{
  "name": "milenstanev-mstanev.angular.1.x.x.core",
  "version": "1.0.0",
  "main": "build.js",
  "license": "MIT",
  "dependencies": {
    "browser-sync": "^2.18.13",
    "gulp": "^3.9.1",
    "jspm": "^0.16.53"
  },
  "jspm": {
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.6.6",
      "angular-animate": "github:angular/bower-angular-animate@^1.6.5",
      "angular-sanitize": "github:angular/bower-angular-sanitize@^1.6.5",
      "angular-strap": "npm:angular-strap@^2.3.12",
      "angular-ui-router": "npm:@uirouter/angularjs@^1.0.6",
      "jirikavi/AngularJS-Toaster": "github:jirikavi/AngularJS-Toaster@^2.1.0",
      "json": "github:systemjs/plugin-json@^0.3.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "scripts": {
    "jspm-install": "./node_modules/.bin/jspm install -y",
    "dev": "node util/start.dev.js",
    "build": "node util/build.js",
    "bundle": "./node_modules/.bin/jspm bundle ./src/index.js --minify --inject --no-mangle",
    "esdoc": "esdoc -c esdoc.json",
    "esdoc-show": "open ./esdoc/index.html"
  }
